## The function is currently defined as
function (p) 
{
    if (!(sum(p >= 1, na.rm = T) + sum(p <= 0, na.rm = T) == 
        0)) 
        warning("logit is only defined between the interval 0 and 1")
    p[p == 0] <- NA
    p[p == 1] <- NA
    return(log(p/(1 - p)))
  }
}
